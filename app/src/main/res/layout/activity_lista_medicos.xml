<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.maishealth.maishealth.usuario.gui.ListaMedicos">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorButoes"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:title="@string/lista_de_medicos"
        app:titleTextColor="@color/colorFundo" />

    <ListView
        android:id="@+id/lstMedicos"
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:dividerHeight="7dp" />

    <ImageView
        android:id="@+id/cancela_1"
        android:layout_width="67dp"
        android:layout_height="67dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:onClick="voltar"
        app:srcCompat="@android:drawable/ic_delete" />

</LinearLayout>
